{
  "name": "ts-notifier",
  "version": "1.1.1",
  "description": "Notifications library written in TypeScript with no dependencies",
  "keywords": [
    "typescript",
    "notifications"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/alxshelepenok/notifier.git"
  },
  "license": "MIT",
  "author": "Alexander Shelepenok <alxshelepenok@gmail.com>",
  "main": "./target/notifier.min.js",
  "typings": "./target/src/index.d.ts",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint:ts": "tslint -c tslint.json \"src/**/*.ts\"",
    "lint:scss": "stylelint \"src/**/*.scss\"",
    "lint": "concurrently \"yarn run lint:ts\" \"yarn run lint:scss\"",
    "clean": "rimraf target",
    "build": "yarn run clean && webpack --mode production",
    "build-dev": "yarn run clean && webpack --mode development"
  },
  "devDependencies": {
    "autoprefixer": "^9.8.8",
    "css-loader": "^5.2.7",
    "node-sass": "^6.0.1",
    "postcss-loader": "^3.0.0",
    "sass-loader": "^10.2.1",
    "style-loader": "^2.0.0",
    "ts-lint": "^4.5.1",
    "ts-loader": "^8.3.0",
    "typescript": "^4.6.3",
    "webpack": "^4.46.0",
    "webpack-cli": "^4.9.2",
    "browserslist": "^4.20.2",
    "mini-css-extract-plugin": "^1.6.2",
    "stylelint": "13.13.1",
    "stylelint-config-recommended-scss": "4.3.0",
    "stylelint-scss": "3.21.0",
    "tslint": "6.1.3",
    "concurrently": "7.1.0",
    "rimraf": "3.0.2",
    "optimize-css-assets-webpack-plugin": "^6.0.1",
    "uglifyjs-webpack-plugin": "^2.2.0"
  }
}
